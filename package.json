    "babel-eslint": "^10.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "framer-motion": "^1.9.0",
    "lodash": "latest",
    "particles-bg": "^2.4.7",
    "prop-types": "latest",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^4.10.1",
    "react-particles-js": "^2.7.1",
    "react-router-dom": "^5.1.2",
    "react-spinners": "^0.8.0",
